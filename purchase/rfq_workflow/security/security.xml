<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.module.category" id="rfq_workflow">
            <field name="name">RFQ WorkFlow</field>
            <field name="sequence">20</field>
        </record>

        <record id="rfq_manager_director_group" model="res.groups">
            <field name="name">مدير الاداره</field>
            <field name="category_id" ref="rfq_workflow"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

        <record id="rfq_general_secretary_group" model="res.groups">
            <field name="name">الامين العام</field>
            <field name="category_id" ref="rfq_workflow"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>


    </data>
</odoo>