<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <odoo>


        <record id="stock_report_search_view" model="ir.ui.view">
            <field name="name">stock.report</field>
            <field name="model">stock.report</field>
            <field name="inherit_id" ref="stock_enterprise.stock_report_search_view"/>
            <field name="arch" type="xml">
                <filter name="done" position="after">
                        <filter string='الجهة الطالبة' name="by_analytic_account_id"
                                context="{'group_by': 'analytic_account_id'}"/>
                        <filter string='الشخص/الشركة' name="by_partner_id"
                                context="{'group_by': 'partner_id'}"/>
                </filter>

            </field>
        </record>

    </odoo>
</openerp>
