<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <odoo>


        <record id="view_stock_quant_tree_inventory_editable" model="ir.ui.view">
            <field name="name">stock.quant.tree</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
            <field name="arch" type="xml">
                <field name="location_id" position="before">
                    <field name="analytic_account_ids" widget="many2many_tags"/>
                </field>
            </field>
        </record>

        <record id="view_stock_quant_search_inventory_editable" model="ir.ui.view">
            <field name="name">stock.quant.search</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.quant_search_view"/>
            <field name="arch" type="xml">
                <filter name="productgroup" position="after">
                    <filter  string='الجهة الطالبة' name="analytic_account_ids"
                            context="{'group_by': 'analytic_account_ids'}"/>
                    <filter string='فئة المنتج' name="product_categ_id"
                            context="{'group_by': 'product_categ_id'}"/>
                    <filter  string='الشخص/الشركة' name="partner_ids"
                            context="{'group_by': 'partner_ids'}"/>
                </filter>
            </field>
        </record>

    </odoo>
</openerp>
