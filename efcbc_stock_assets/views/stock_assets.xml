<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="fixed_assets_picking_type_action" model="ir.actions.act_window">
        <field name="name">الاصول الثابتة</field>
        <field name="res_model">stock.picking.type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">kanban,form</field>
        <field name="domain">[('fixed_assets','=',True)]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                لايوجد مخازن للاصول الثابتة
            </p>
        </field>
    </record>

    <record id="stock.stock_picking_type_action" model="ir.actions.act_window">
        <field name="domain">[('fixed_assets','=',False)]</field>
    </record>

    <menuitem action="fixed_assets_picking_type_action" id="fixed_assets_picking"  sequence="200"
              web_icon="efcbc_stock_assets,static/description/icon.png"/>

</odoo>
