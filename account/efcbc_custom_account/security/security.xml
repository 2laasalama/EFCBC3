<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record model="ir.module.category" id="efcbc_account_category">
        <field name="name">EFCBC Account</field>
        <field name="sequence">101</field>
    </record>


    <record id="group_treasury_user" model="res.groups">
        <field name="name">Treasury User</field>
        <field name="category_id" ref="efcbc_account_category"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>

    </record>

</odoo>